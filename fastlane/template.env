# App Store Connect API Key (Required)
API_KEY_ID=your_api_key_id_here
API_ISSUER_ID=your_issuer_id_here
API_KEY_BASE64=your_base64_encoded_p8_key_here

# Apple Developer Account (Required)
FASTLANE_USERNAME=your_apple_id@example.com
DEVELOPMENT_TEAM=your_team_id_here

# App Configuration (Optional)
APP_IDENTIFIER=com.othmanshahrouri.cd.starter.project

# Match Configuration (Optional - for certificate management)
MATCH_GIT_URL=git@github.com:yourusername/certificates.git
